<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>userView</managed-bean-name>
  <managed-bean-class>com.UserView</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>pages/home/home.xhtml</display-name>
  <from-view-id>/pages/home/home.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{navigator.navigateTo}</from-action>
   <from-outcome>Secured</from-outcome>
   <to-view-id>/pages/secure/secured.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>pages/home/home.xhtml</display-name>
  <from-view-id>/pages/home/home.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{navigator.navigateTo}</from-action>
   <from-outcome>UnSecured</from-outcome>
   <to-view-id>/pages/insecure/unsecured.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
 </application>
</faces-config>
